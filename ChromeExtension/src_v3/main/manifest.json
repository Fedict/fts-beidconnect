{
   "key": "",
    "name": "BeIDConnect",
    "version": "@@VERSION",
    "minimum_chrome_version": "88.0",
    "manifest_version": 3,
    "description": "Use your Belgian eID on DSS secure platforms (DSS, IDP)",
    "icons": {
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [{
        "matches": ["*://*/*", "file:///*"],
        "js": ["content.js"],
        "run_at": "document_start",
        "all_frames":true
    }],
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [{
        "resources": ["page.js"],
        "matches": ["*://*/*", "file:///*"]
    }],
    "permissions": ["nativeMessaging"]
}
